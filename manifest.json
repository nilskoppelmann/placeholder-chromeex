{
    "manifest_version": 2,
    "name": "Nils Koppelmann Chrome Extension",
    "version": "1.0",

    "description": "",
    "homepage_url": "http://nils-koppelmann.de/",
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false 
